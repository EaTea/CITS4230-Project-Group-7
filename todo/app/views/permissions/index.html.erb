<div class="doc">

<h1>Users who can access <%= List.find_by_id(session[:list_id]).name%> </h1>

<table class="tlist">
	<tr class="tlist_headings">
		<th>User</th>
		<th></th>
	</tr>

<% @permissions.each do |permission| %>
	<tr class="tlist_row">
		<td><%= permission.user.username %></td>
		<td><%= link_to 'See Permissions', permission %></td>
	</tr>
<% end %>
</table>

<br />

<%= link_to 'Add a new user', new_permission_path %>
</div>
